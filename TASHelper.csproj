<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net4.5.2</TargetFramework>
	<LangVersion>10.0</LangVersion>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Authors>$(AssemblyName)</Authors>
    <RootNamespace>Celeste.Mod.TASHelper</RootNamespace>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <WarningLevel>4</WarningLevel>
    <NoWarn>CS8618</NoWarn>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <WarningLevel>4</WarningLevel>
    <NoWarn>CS8618</NoWarn>
  </PropertyGroup>

  <ItemDefinitionGroup>
    <PackageReference IncludeAssets="compile" />
    <Reference>
      <Private>False</Private>
    </Reference>
  </ItemDefinitionGroup>

  <ItemGroup>
    <Reference Include="BrokemiaHelper">
      <HintPath>Libs\BrokemiaHelper.dll</HintPath>
    </Reference>
    <Reference Include="Celeste">
      <HintPath>Libs\Celeste.exe</HintPath>
    </Reference>
    <Reference Include="CelesteTAS-EverestInterop">
      <HintPath>Libs\CelesteTAS-EverestInterop.dll</HintPath>
      <Private>False</Private>
    </Reference>
    <Reference Include="ChronoHelper">
      <HintPath>Libs\ChronoHelper.dll</HintPath>
    </Reference>
    <Reference Include="ExtendedVariantMode">
      <HintPath>Libs\ExtendedVariantMode.dll</HintPath>
    </Reference>
    <Reference Include="FNA">
      <HintPath>Libs\FNA.dll</HintPath>
    </Reference>
    <Reference Include="FrostTempleHelper">
      <HintPath>Libs\FrostTempleHelper.dll</HintPath>
    </Reference>
    <Reference Include="IsaMods">
      <HintPath>Libs\IsaMods.dll</HintPath>
    </Reference>
    <Reference Include="MMHOOK_Celeste">
      <HintPath>Libs\MMHOOK_Celeste.dll</HintPath>
    </Reference>
    <PackageReference Include="MonoMod.RuntimeDetour" Version="22.01.04.03" />
    <Reference Include="PandorasBox">
      <HintPath>Libs\PandorasBox.dll</HintPath>
    </Reference>
    <Reference Include="System.Net.Http" />
    <Reference Include="VivHelper">
      <HintPath>Libs\VivHelper.dll</HintPath>
    </Reference>
  </ItemGroup>

  <ItemGroup>
    <Compile Remove="Libs\**" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Remove="Libs\**" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="Libs\**" />
  </ItemGroup>
</Project>
